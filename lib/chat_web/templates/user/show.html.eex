<h1>Show User</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>
</ul>

<span><%= link "Editar", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Voltar", to: Routes.user_path(@conn, :index) %></span>
